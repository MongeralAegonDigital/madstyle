- var atoms = public.source.style["04-base"]
- var molecules = public.source.style["05-objects"]
- var organisms = public.source.style["06-components"]

main.mad-content
	section
		h2 Átomos
		each value, key in atoms
			section.mad-atom(id='#{key}')
				!=partial("source/style/04-base/"+key+"/"+key)

	section
		h2 Moléculas
		each value, key in molecules
			section.mad-molecule(id='#{key}')
				!=partial("source/style/05-objects/"+key+"/"+key)

	section
		h2 Organismos
		each value, key in organisms
			section.mad-organism(id='#{key}')
				!=partial("source/style/06-components/"+key+"/"+key)

